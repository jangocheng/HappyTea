<view class='wx_dialog_container' hidden="{{!isShow}}">
    <view class='wx-mask'></view>
    <view class='wx-dialog'>
        <view class='wx-dialog-title' style='color:titleTextColor'>{{ title }}</view>
        <view class='wx-dialog-content' style='color:contentTextColor'>{{ content }}</view>
        <view class='wx-dialog-footer'>
          <view class='wx-dialog-btn' catchtap='_cancelEvent'style='color:cancelTextColor'>{{ cancelText }}</view>
          <view class='wx-dialog-btn' catchtap='_confirmEvent' style='color:confirmTextColor'>{{ confirmText }}</view>
        </view>
    </view>
</view>